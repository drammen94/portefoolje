{
	"name": "PipelineCopyReturnFileDnB",
	"properties": {
		"activities": [
			{
				"name": "CopyFilefromInboxDnB",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "SftpReadSettings",
							"recursive": true,
							"disableChunking": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "srcFileDnB",
						"type": "DatasetReference",
						"parameters": {
							"DisplayName": "@pipeline().parameters.DisplayName"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "targetFolder",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"Displayname": {
				"type": "string",
				"defaultValue": "@pipeline().parameters.DisplayName"
			}
		},
		"annotations": []
	}
}